{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.cs"],
          "src": "../com.sushiwaumai.chronity"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": true
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["manual/**.md", "manual/**/toc.yml"]
      }, 
			{
				"files": ["index.md", "toc.yml"]
			}
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "../docs",
    "globalMetadata": {
      "_appTitle": "Chronity",
      "_appFooter": "Chronity",
      "_enableSearch": true,
      "_appLogoPath": "images/logo.svg"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
